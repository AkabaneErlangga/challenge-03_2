<div class="col-4 mb-4">
    <div class="card">
        <img src="images/Car.png" class="card-img-top" alt="...">
        <div class="card-body">
            <h6 class="card-title"><%= car.name %> </h6>
            <h6 class="card-title">Rp. <%= car.price %>  / hari</h6>
            <p class="card-text">
                <i class="fa-solid fa-key"></i>
                <%= car.startRent %>  - <%= car.finishRent %> 
            </p>
            <p class="card-text">
                <i class="fa-regular fa-clock"></i>
                Updated at <%= car.updatedAt %> , 09.00
            </p>
            <div class="row">
                <div class="col-6">
                    <a href="#" class="btn btn-outline-success w-100">
                        <i class="fa-regular fa-trash-can"></i>
                        Delete
                    </a>
                </div>
                <div class="col-6">
                    <a href="#" class="btn btn-success w-100">
                        <i class="fa-regular fa-pen-to-square"></i>
                        Edit
                    </a>
                </div>
            </div>
        </div>
    </div>
</div>